<h2>Ingredients for <%= meal %></h2>
<ul>
  <% products.forEach(p => { %>
    <li>
      <strong><%= p.name %></strong><br>
      <%= p.description %> - $<%= p.price.toFixed(2) %><br>
      <% if (p.image) { %>
        <img src="<%= p.image %>" alt="<%= p.name %>" style="height: 100px;">
      <% } %>
    </li>
  <% }) %>
</ul>
